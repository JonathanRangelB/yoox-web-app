{
  "name": "yoox-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "env": "node ./scripts/set-env.js",
    "start": "pnpm run env && ng serve --configuration development",
    "build": "pnpm run env && ng build --configuration production",
    "build:purgecss": "pnpm run env && ng build --configuration production",
    "watch": "pnpm run env && ng build --watch --configuration development",
    "test": "pnpm run env && ng test --configuration development",
    "commitlint": "commitlint --edit",
    "husky:init": "npx husky-init && rm .husky/pre-commit && chmod ug+x .husky/*",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint:ci": "ng lint --fix",
    "test:ci": "ng test --browsers ChromeHeadless --watch false",
    "e2e:ci": "ng e2e --watch false --headless",
    "all:ci": "pnpm run env && npm run lint:ci && npm run test:ci && npm run e2e:ci",
    "prepare": "husky install",
    "postbuild": "node ./scripts/purgecss.mjs",
    "httpserver": "pnpm dlx http-server dist/yoox-web-app/browser/ -p 4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.5",
    "@angular/cdk": "^19.2.8",
    "@angular/common": "^19.2.5",
    "@angular/compiler": "^19.2.5",
    "@angular/core": "^19.2.5",
    "@angular/forms": "^19.2.5",
    "@angular/platform-browser": "^19.2.5",
    "@angular/platform-browser-dynamic": "^19.2.5",
    "@angular/router": "^19.2.5",
    "@angular/service-worker": "^19.2.5",
    "@typescript-eslint/types": "^8.29.0",
    "@typescript-eslint/utils": "^8.29.0",
    "primeflex": "^3.3.1",
    "primeicons": "^7.0.0",
    "primeng": "^17.18.15",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.6",
    "@angular-eslint/builder": "19.3.0",
    "@angular-eslint/eslint-plugin": "19.3.0",
    "@angular-eslint/eslint-plugin-template": "19.3.0",
    "@angular-eslint/schematics": "19.3.0",
    "@angular-eslint/template-parser": "19.3.0",
    "@angular/cli": "^19.2.6",
    "@angular/compiler-cli": "^19.2.5",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@cypress/schematic": "^3.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.23.0",
    "@schematics/angular": "^19.2.6",
    "@types/jasmine": "~5.1.7",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^14.2.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.5.3",
    "prettier-eslint": "^16.3.0",
    "purgecss": "^7.0.2",
    "typescript": "~5.8.2"
  },
  "pnpm": {
    "overrides": {
      "body-parser@<1.20.3": ">=1.20.3",
      "send@<0.19.0": ">=0.19.0",
      "serve-static@<1.16.0": ">=1.16.0",
      "express@<4.20.0": ">=4.20.0",
      "path-to-regexp@<0.1.10": ">=0.1.10",
      "rollup@>=4.0.0 <4.22.4": ">=4.22.4",
      "cookie@<0.7.0": ">=0.7.0"
    }
  }
}
