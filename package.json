{
  "name": "yoox-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "env": "node ./scripts/set-env.js",
    "start": "pnpm run env && ng serve --configuration development",
    "build": "pnpm run env && ng build --configuration production",
    "build:purgecss": "pnpm run env && ng build --configuration production && pnpm run purgecss",
    "watch": "pnpm run env && ng build --watch --configuration development",
    "test": "pnpm run env && ng test --configuration development",
    "commitlint": "commitlint --edit",
    "husky:init": "npx husky-init && rm .husky/pre-commit && chmod ug+x .husky/*",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint:ci": "ng lint --fix",
    "test:ci": "ng test --browsers ChromeHeadless --watch false",
    "e2e:ci": "ng e2e --watch false --headless",
    "all:ci": "pnpm run env && npm run lint:ci && npm run test:ci && npm run e2e:ci",
    "prepare": "husky install",
    "purgecss": "node scripts/purgecss.js",
    "httpserver": "pnpm dlx http-server dist/yoox-web-app/browser/ -p 4200"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.2",
    "@angular/cdk": "^18.2.2",
    "@angular/common": "^18.2.2",
    "@angular/compiler": "^18.2.2",
    "@angular/core": "^18.2.2",
    "@angular/forms": "^18.2.2",
    "@angular/platform-browser": "^18.2.2",
    "@angular/platform-browser-dynamic": "^18.2.2",
    "@angular/router": "^18.2.2",
    "@angular/service-worker": "^18.2.2",
    "primeflex": "^3.3.1",
    "primeicons": "^6.0.1",
    "primeng": "^17.18.9",
    "rxjs": "~7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.2",
    "@angular-eslint/builder": "18.3.0",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular-eslint/eslint-plugin-template": "18.3.0",
    "@angular-eslint/schematics": "18.3.0",
    "@angular-eslint/template-parser": "18.3.0",
    "@angular/cli": "^18.2.2",
    "@angular/compiler-cli": "^18.2.2",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@cypress/schematic": "^2.5.2",
    "@schematics/angular": "^16.2.15",
    "@types/jasmine": "~4.3.6",
    "@types/node": "^20.16.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "cypress": "^13.14.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.1",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "purgecss": "^6.0.0",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.5.4"
  }
}
